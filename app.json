{
  "expo": {
    "name": "AudioTour",
    "slug": "AudioTour",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "audiotour",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.woutervanderlaan.audiotour",
      "appleTeamId": "WNTLECWTLQ"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.woutervanderlaan.audiotour",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png"
        }
      ],
      [
        "expo-location",
        {
          "isIosBackgroundLocationEnabled": false,
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to access your location to detect nearby museums.",
          "locationWhenInUsePermission": "Allow $(PRODUCT_NAME) to access your location to detect nearby museums."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos for uploads.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to take photos.",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone for video audio."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "805b6cbc-bd2c-4ce7-88a9-390ed3c063c1"
      }
    },
    "owner": "woutervanderlaan"
  }
}
