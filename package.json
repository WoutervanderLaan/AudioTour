{
  "name": "audiotour",
  "version": "1.0.0",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "start": "APP_VARIANT=development expo start --dev-client",
    "start:reset:cache": "APP_VARIANT=development expo start --reset-cache",
    "web": "APP_VARIANT=development expo start --web",
    "android": "APP_VARIANT=development expo run:android",
    "ios": "APP_VARIANT=development expo run:ios --device",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "prepare": "husky",
    "clean": "react-native-clean-project",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run format",
    "test": "jest --watchAll",
    "storybook-generate": "sb-rn-get-stories",
    "storybook": "APP_VARIANT=development storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@hookform/resolvers": "^5.2.2",
    "@react-navigation/bottom-tabs": "^7.8.11",
    "@react-navigation/elements": "^2.6.4",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@tanstack/react-query": "^5.90.2",
    "expo": "54.0.22",
    "expo-asset": "~12.0.8",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.17",
    "expo-dev-menu": "^7.0.17",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-location": "^19.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-system-ui": "~6.0.8",
    "fast-text-encoding": "^1.0.6",
    "immer": "^10.1.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-native": "0.81.5",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-nitro-modules": "^0.31.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-unistyles": "^3.0.17",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-web": "^0.21.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.38.0",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.5.0",
    "@react-native-community/slider": "^5.1.1",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-ondevice-actions": "^10.0.7",
    "@storybook/addon-ondevice-controls": "^10.0.7",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/react-native": "^10.0.7",
    "@storybook/react-native-web-vite": "^10.0.8",
    "@tanstack/eslint-plugin-query": "^5.90.1",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-loader": "^8.4.1",
    "eslint": "^9.38.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-storybook": "^10.0.8",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.13",
    "lint-staged": "^16.2.6",
    "msw": "^2.12.2",
    "prettier": "^3.6.2",
    "react-native-clean-project": "^4.0.3",
    "react-native-reanimated": "^4.1.5",
    "react-native-svg": "^15.15.0",
    "storybook": "^10.0.8",
    "typescript": "~5.9.2",
    "vite": "^7.2.2",
    "vite-plugin-babel": "^1.3.2"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/.jest/jest-init.js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native|expo|msw|until-async)"
    ]
  }
}
